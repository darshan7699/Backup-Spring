<constructor-arg> <value>10</value> </constructor-arg>
<constructor-arg> <value>Devlopment</value> </constructor-arg>
<constructor-arg> <ref bean="adress"/> </constructor-arg>
</bean>

<bean name="adress" class="com.te.springcoret2.Adress">
